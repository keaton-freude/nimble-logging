{
    "name": "nimble-logging",
    "version": "1.0.5",
    "description": "Logger",
    "main": "dist/nimble-logging.js",
    "types": "dist/nimble-logging.d.ts",
    "scripts": {
        "test": "mocha -r ts-node/register src/**/*.spec.ts",
        "test:watch": "mocha --watch --watch-files=src/ -r ts-node/register src/**/*.spec.ts",
        "test-debug": "mocha --inspect -r ts-node/register src/**/*.spec.ts",
        "test-debug:watch": "mocha --inspect --watch --watch-files=src/ -r ts-node/register src/**/*.spec.ts",
        "build": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keaton-freude/nimble-logging.git"
    },
    "keywords": [
        "log",
        "logger",
        "logging",
        "nimble"
    ],
    "author": "Keaton Freude",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/keaton-freude/nimble-logging/issues"
    },
    "homepage": "https://github.com/keaton-freude/nimble-logging#readme",
    "files": [
        "dist/**/*"
    ],
    "devDependencies": {
        "@types/chai": "^4.2.9",
        "@types/mocha": "^7.0.1",
        "chai": "^4.2.0",
        "mocha": "^7.0.1",
        "prettier": "^1.19.1",
        "ts-node": "^8.6.2",
        "typescript": "^3.8.2"
    }
}
